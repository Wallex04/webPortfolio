import { Formik, Field, Form, ErrorMessage } from 'formik';
import * as Yup from 'yup';
import textInput from '../../components/Input/TextInput'

const ContactUs = () => {
  const initialValues = {
    firstName: '',
    lastName: '',
    email: '',
    phoneNumber: '',
    message: '',
  };

  const validationSchema = Yup.object().shape({
    firstName: Yup.string().required('First name is required'),
    lastName: Yup.string().required('Last name is required'),
    email: Yup.string().email('Invalid email address').required('Email is required'),
    phoneNumber: Yup.string().matches(/^[0-9+]*$/, 'Invalid phone number').required('Phone number is required'),
    message: Yup.string().required('Message is required'),
  });

  const onSubmit = (values, { setSubmitting, setStatus }) => {
    
    setTimeout(() => {
      const errors = {};

      if (!values.firstName) {
        errors.firstName = 'First name is required';
      }

      if (!values.lastName) {
        errors.lastName = 'Last name is required';
      }

      if (!values.email) {
        errors.email = 'Email is required';
      } else if (!/^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i.test(values.email)) {
        errors.email = 'Invalid email address';
      }

      if (!values.phoneNumber) {
        errors.phoneNumber = 'Phone number is required';
      } else if (!/^[0-9+]*$/.test(values.phoneNumber)) {
        errors.phoneNumber = 'Invalid phone number';
      }

      if (!values.message) {
        errors.message = 'Message is required';
      }

      // If there are errors, set them to the formik errors object
      if (Object.keys(errors).length > 0) {
        setSubmitting(false);
        setStatus({ success: false, message: 'Form submission failed. Please check the errors.' });
        return;
      }

      //proceeding without errors 
      console.log('Form data submitted successfully:', values);
      setSubmitting(false);
      setStatus({ success: true, message: 'Form submitted successfully!' });
    }, 500); 
  };

  return (
    <div className="w-full h-full flex justify-center items-center p-4">
      <Formik initialValues={initialValues} validationSchema={validationSchema} onSubmit={onSubmit}>
        {({ status }) => (
          <Form className="flex flex-col max-w-[800px] w-full">
            <p className="text-4xl font-bold text-center pb-20">Contact Us.</p>
            <div className="shadow-md rounded-lg p-6">
              <div className="flex flex-col lg:flex-row gap-4 w-full">
                <Field name="firstName">
                  {({ field }) => (
                    <div className="flex flex-col w-full">
                      <label className="mb-2 font-semibold">First name</label>
                      <input
                        {...field}
                        className="bg-[#f6f7f8] p-2 rounded-lg border border-gray-300 w-full"
                        type="text"
                        placeholder="Bonnie"
                      />
                      <ErrorMessage name="firstName" component="div" className="text-red-500" />
                    </div>
                  )}
                </Field>

                <Field name="lastName">
                  {({ field }) => (
                    <div className="flex flex-col w-full">
                      <label className="mb-2 font-semibold">Last name</label>
                      <input
                        {...field}
                        className="bg-[#f6f7f8] p-2 rounded-lg border border-gray-300 w-full mb-2"
                        type="text"
                        placeholder="Green"
                      />
                      <ErrorMessage name="lastName" component="div" className="text-red-500" />
                    </div>
                  )}
                </Field>
              </div>

              <div className="flex flex-col lg:flex-row gap-4 w-full">
                <Field name="email">
                  {({ field }) => (
                    <div className="flex flex-col w-full">
                      <label className="mb-2 font-semibold">Email</label>
                      <input
                        {...field}
                        className="bg-[#f6f7f8] p-2 rounded-lg border border-gray-300 w-full"
                        type="text"
                        placeholder="name@example.com"
                      />
                      <ErrorMessage name="email" component="div" className="text-red-500" />
                    </div>
                  )}
                </Field>

                <Field name="phoneNumber">
                  {({ field }) => (
                    <div className="flex flex-col w-full">
                      <label className="mb-2 font-semibold">Phone number</label>
                      <input
                        {...field}
                        className="bg-[#f6f7f8] p-2 rounded-lg border border-gray-300 w-full mb-2"
                        type="number"
                        placeholder="+(12) 345 6789"
                      />
                      <ErrorMessage name="phoneNumber" component="div" className="text-red-500" />
                    </div>
                  )}
                </Field>
              </div>

              <h1 className="mb-2 font-semibold">Your Message</h1>
              <Field name="message">
                {({ field }) => (
                  <textarea
                    {...field}
                    className="bg-[#f6f7f8] p-2 rounded-lg border border-gray-300 w-full"
                    name="message"
                    rows="10"
                    placeholder="Message"
                  />
                )}
              </Field>
              <ErrorMessage name="message" component="div" className="text-red-500" />

              {status && status.success !== undefined && (
                <div className={status.success ? 'text-green-500' : 'text-red-500 mb-4'}>
                  {status.message}
                </div>
              )}

              <button type="submit" className="text-white border-2 px-4 py-3 my-4 w-[150px] rounded-xl bg-[#D43325]">
                Get started
              </button>
            </div>
          </Form>
        )}
      </Formik>
    </div>
  );
};

export default ContactUs;






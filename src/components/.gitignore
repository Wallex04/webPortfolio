import { useState } from "react";
import { NavLink } from 'react-router-dom';
import Button from "../components/Button";
import {Logo, MenuBar} from "../components/svgFolder/Svg";

function Header() {
    const [isNavOpen, setIsNavOpen] = useState(false);
 
    return (
        <div className="font-sora">
            <div className="flex items-center bg-black gap-3 lg:gap-11 border-gray-400 pt-6 lg:px-5">
                <NavLink to="/">
                    <Logo/>
                </NavLink>
                <NavLink to="/" className="text-[hsla(5,70%,49%,1)] text-sm border-b-2 pb-2 border-red-600 font-medium">Candidates</NavLink>
                <NavLink to="/" className="text-[hsla(0,0%,100%,1)] text-sm font-normal pb-2">Organizations</NavLink>
            </div>
 
            <nav className="mt-3">
                <section className="MOBILE-MENU flex lg:hidden">
                    <div
                        className="HAMBURGER-ICON absolute top-0 right-0 px-2 mb-6 py-7"
                        onClick={() => setIsNavOpen((prev) => !prev)}
                    >
                        <span className="block h-0.5 w-6 animate-pulse mb-2 bg-white"></span>
                        <span className="block h-0.5 w-6 animate-pulse mb-2 bg-white"></span>
                        <span className="block h-0.5 w-6 animate-pulse mb-2 bg-white"></span>
                    </div>
                    <div className={isNavOpen ? "showMenuNav" : "hideMenuNav"}>
                        <div
                            className="CROSS-ICON absolute top-0 right-0 px-8 py-8 "
                            onClick={() => setIsNavOpen(false)} >
                            <MenuBar/>
                      </div>
                        
                        <ul className="MENU-LINK-MOBILE-OPEN flex flex-col items-center min-h-[250px] -mt-28">
                            <li className="my-6 uppercase">
                                <NavLink to="/about" className='hover:text-red-500 text-[#53555A] font-semibold' onClick={() => setIsNavOpen(false)}>About i-academy</NavLink>
                            </li>
                            <li className="my-6 uppercase">
                                <NavLink className='hover:text-red-500 text-[#53555A] font-semibold' to='/opportunities' onClick={() => setIsNavOpen(false)}>Tech opportunities</NavLink>
                            </li>
                            <li className="my-6 uppercase">
                                <NavLink className='hover:text-red-500 text-[#53555A] font-semibold' to='/faqs' onClick={() => setIsNavOpen(false)}>FAQs</NavLink>
                            </li>
                            <li className="my-6 uppercase">
 
                                <NavLink className='hover:text-red-500 text-[#53555A] font-semibold' to='/contact' onClick={() => setIsNavOpen(false)}>contact us</NavLink>
                            </li>
                            <Button
                                filled
                                label="Get started"
                                className="text-sm my-6 font-medium py-3 bg-red-600 px-5 text-[hsla(223,4%,34%,1)] border-[hsla(223,4%,34%,1)]"
                            />
                        </ul>
                    </div>
                </section>
                <ul className="DESKTOP-MENU hidden font-inter lg:flex items-center justify-end mr-8 mt-8">
                    <li>
                        <NavLink to="/about" className=' hover:text-red-500 text-[#53555A] font-semibold md:p-3'>About i-academy</NavLink>
                    </li>
                    <li>
                        <NavLink className=' hover:text-red-500 text-[#53555A] font-semibold md:p-3' to='/opportunities'>Opportunities</NavLink>
                    </li>
                    <li>
                        <NavLink className=' hover:text-red-500 text-[#53555A] font-semibold md:p-3' to='/faqs'>FAQs</NavLink>
                    </li>
                    <li>
                        <NavLink className=' hover:text-red-500 text-[#53555A] font-semibold md:p-3' to='/contact'>contact us</NavLink>
                    </li>
                    <Button
                    filled
                        label="Get started"
                        className="text-sm font-medium py-3 px-5 ml-3 text-[hsla(223,4%,34%,1)] border-[hsla(223,4%,34%,1)] bg-red-600"
                    />
                </ul>
            </nav>
            <style>{`
        .hideMenuNav {
          display: none;
        }
        .showMenuNav {
          display: block;
          position: absolute;
          width: 100%;
          height: 100vh;
          top: 0;
          left: 0;
          background: white;
          z-index: 10;
          display: flex;
          flex-direction: column;
          justify-content: space-evenly;
          align-items: center;
        }
      `}</style>
        </div>
    );
}

export default Header;



